{
  "name": "storion-be",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "dev": "sls offline start",
    "redeploy:staging": "sls deploy --stage dev",
    "redeploy:production": "sls deploy --stage prod",
    "deploy:staging": "sls deploy function -f app --stage dev",
    "deploy:production": "sls deploy function -f app --stage prod",
    "migrate": "knex migrate:latest --knexfile ./knex/knexfile.js",
    "seed": "knex seed:run --knexfile ./knex/knexfile.js",
    "remigrate": "knex-migrate redo --verbose --knexfile ./knex/knexfile.js & yarn seed",
    "destroy": "node scripts/destroy",
    "reset": "yarn destroy && yarn migrate && yarn seed && yarn dev",
    "tail:staging": "serverless logs -f app -t --stage dev",
    "tail:production": "serverless logs -f app -t --stage prod"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "helmet": "^3.14.0",
    "jwks-rsa": "^1.3.0",
    "knex": "^0.15.2",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "pg": "^7.6.0",
    "serverless": "^1.35.1",
    "serverless-http": "^1.8.0"
  },
  "devDependencies": {
    "knex-cleaner": "^1.1.4",
    "knex-migrate": "^1.7.0",
    "nodemon": "^1.18.5",
    "serverless-offline": "^3.31.3",
    "serverless-plugin-include-dependencies": "^3.2.1"
  }
}
